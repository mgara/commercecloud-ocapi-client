{
    "env": {
        "production": {
            "presets": [
                ["env", {"modules": false}]
            ],
            "plugins": [
                "external-helpers",
                "transform-class-properties",
                ["transform-object-rest-spread", { "useBuiltIns": true }]
            ]
        },
        "test": {
            "presets": ["env"],
            "plugins": [
                "transform-class-properties",
                ["transform-object-rest-spread", { "useBuiltIns": true }]
            ]
        }
    }
}
